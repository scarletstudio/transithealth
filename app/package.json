{
  "name": "transithealth-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8001",
    "build": "next build",
    "start": "next start -p 8001",
    "build:ci": "echo \"NEXT_PUBLIC_API=https://transithealth.herokuapp.com\" > .env.local && next build && next export -o transithealth",
    "start:ci": "python3 -m http.server 8001",
    "export": "next export -o transithealth",
    "clean": "rm -rf build && rm -rf transithealth",
    "cy:run": "cypress run",
    "cy:run-example": "cypress run --spec 'tests/e2e/integration/sample_spec.js'",
    "cy:test-timeline" : "cypress run --spec 'tests/e2e/integration/timeline_test.js'"
  },
  "dependencies": {
    "d3-geo": "^2.0.1",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recharts": "^2.0.9",
    "throttle-debounce": "^3.0.1",
    "use-http": "^1.0.21",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "cypress": "^8.1.0"
  }
}
